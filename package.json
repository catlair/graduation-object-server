  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "dev": "pnpm run start:dev",
    "start:debug": "nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prepare": "husky install",
    "db:reset": "dotenv-flow -- npx prisma migrate reset --preview-feature",
    "db:migrate": "dotenv-flow -- npx prisma migrate dev --preview-feature",
    "db:deploy": "dotenv-flow -- npx prisma migrate deploy --preview-feature",
    "db:g": "dotenv-flow -- npx prisma generate",
    "db:seed": "dotenv-flow -- ts-node prisma/seed.ts",
    "db:demo": "dotenv-flow -- ts-node prisma/demo.ts",
    "db:studio": "dotenv-flow -- npx prisma studio"
  },
